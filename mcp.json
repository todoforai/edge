{
  "mcpServers": {
    "havliktomi@gmail": {
      "command": "npx",
      "args": [
        "-y",
        "@todoforai/server-gmail-autoauth-mcp"
      ],
      "env": {
        "GMAIL_CREDENTIALS_PATH": "~/.gmail-mcp/credentials-havliktomi-gmail.json"
      }
    },
    "mongodb": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-mongo-server"
