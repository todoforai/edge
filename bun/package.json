{
  "name": "@todoforai/edge",
  "version": "0.12.6",
  "type": "module",
  "bin": {
    "todoforai-edge": "dist/index.js"
  },
  "files": ["dist"],
  "scripts": {
    "build": "bun build src/index.ts --target=node --outdir=dist --format=esm && echo '#!/usr/bin/env node' | cat - dist/index.js > dist/tmp && mv dist/tmp dist/index.js && chmod +x dist/index.js",
    "start": "bun run src/index.ts",
    "dev": "bun --watch run src/index.ts --api-key $TODOFORAI_API_KEY_DEV --api-url http://localhost:4000",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "node-pty": "^1.1.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.12",
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0"
  }
}
